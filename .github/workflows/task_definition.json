{
    "containerDefinitions": [{
        "name": "crankbit-backend-main",
        "image": "349498089405.dkr.ecr.ap-southeast-2.amazonaws.com/crankbit-backend-main:latest",
        "cpu": 1024,
        "memory": 2048,
        "portMappings": [{
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp"
        }],
        "essential": true,
        "environment": [],
        "mountPoints": [],
        "volumesFrom": [],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
